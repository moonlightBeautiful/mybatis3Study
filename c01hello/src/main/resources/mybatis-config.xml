<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties resource="jdbc.properties"/>
    <typeAliases>
        <!-- 别名的二种方式 扫描model所在包默认别名、对指定model起别名、-->
        <typeAlias alias="Student" type="com.java1234.model.Student"/>
        <!-- <package name="com.java1234.*"/>-->
        <!--
         <typeAlias alias="Student02" type="com.java1234.c02basicCrud.model.Student"/>
         <typeAlias alias="Student" type="com.java1234.c03relation.sec01yiVSyimodel.Student"/>
         <typeAlias alias="Address" type="com.java1234.c03relation.sec01yiVSyimodel.Address"/>-->
        <!-- <typeAlias alias="Student" type="com.java1234.c03relation.sec02yiVSduo.model.Student" />
         <typeAlias alias="Grade" type="com.java1234.c03relation.sec02yiVSduo.model.Grade"/>-->
        <!-- <typeAlias alias="Student" type="com.java1234.c04dynamicSql.model.Student"/>-->
        <!--<typeAlias alias="User" type="com.java1234.c05sundry.model.User"/>-->
        <!-- <typeAlias alias="Dog" type="com.java1234.c06annotate.model.Dog"/>
         <typeAlias alias="Book" type="com.java1234.c06annotate.model.Book"/>
         <typeAlias alias="Author" type="com.java1234.c06annotate.model.Author"/>-->
        <!--<package name="com.java1234.c06annotate.model.*"/>-->
    </typeAliases>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="${jdbc.driverClassName}"/>
                <property name="url" value="${jdbc.url}"/>
                <property name="username" value="${jdbc.username}"/>
                <property name="password" value="${jdbc.password}"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <!-- 注册映射mapper的三种方式：扫描Mapper所在包默认别名、加载xml、加载mapper类、-->
        <mapper resource="mappers/StudentMapper.xml"/>
        <!--<package name="com.daily.mapper"/>-->
        <!--
         <mappers resource="mappers/c02/StudentMapper.xml"/>
         <mappers resource="mappers/c03/sec01/StudentMapper.xml"/>
         <mappers resource="mappers/c03/sec01/AddressMapper.xml"/>
         <mappers resource="mappers/c03/sec02/StudentMapper.xml"/>
         <mappers resource="mappers/c03/sec02/GradeMapper.xml"/>
        <mapper resource="mappers/c04/StudentMapper.xml"/>-->
        <!--<mapper resource="mappers/c05/UserMapper.xml"/>-->
        <!--<mapper class="com.java1234.c06annotate.mappers.DogMapper"/>
        <mapper class="com.java1234.c06annotate.mappers.BookMapper"/>
        <mapper class="com.java1234.c06annotate.mappers.AuthorMapper"/>
        <mapper class="com.java1234.c06annotate.mappers.BookTypeMapper"/>-->
     <!--   <package name="com.java1234.c06annotate.mappers"/>-->
    </mappers>
</configuration>